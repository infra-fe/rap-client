{
  "name": "rap-client",
  "version": "1.0.0",
  "description": "rap-client react app",
  "main": "app.js",
  "homepage": "https://infra-fe.github.io/rap-client/",
  "scripts": {
    "build-css": "node ./node_modules/sass/sass src/:src/ --quiet",
    "watch-css": "npm run build-css && node ./node_modules/sass/sass src/:src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js --max-old-space-size=4096",
    "build": "npm run build-css && react-scripts build",
    "test-backup": "react-scripts test --env=jsdom",
    "test": "tsc && npm run lint",
    "lint": "echo \"lint checking...\" && eslint . --ext .ts,.tsx && echo \"tsc checking...\"",
    "eject": "react-scripts eject",
    "build-docker": "docker build --rm -f \"Dockerfile\" -t rapteam/rap2-dolores:latest .",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:infra-fe/rap-client.git"
  },
  "author": "Libby, Bosn",
  "license": "ISC",
  "dependencies": {
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.4",
    "@mui/material": "^5.0.6",
    "animate.css": "4.1.1",
    "awesome-debounce-promise": "^2.1.0",
    "chart.js": "^3.6.0",
    "classnames": "^2.3.1",
    "clipboard-copy": "^4.0.1",
    "codemirror": "5.63.3",
    "connected-react-router": "^6.9.1",
    "debounce-promise": "^3.1.2",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "graceful": "1.0.2",
    "history": "^4.10.1",
    "i18next": "^21.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "jquery": "^3.6.0",
    "json5": "^2.2.0",
    "koa": "2.13.4",
    "koa-router": "10.1.1",
    "koa-session": "6.2.0",
    "koa-static": "5.0.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "mockjs": "1.1.0",
    "moment": "^2.29.1",
    "node-fetch": "3.0.0",
    "normalizr": "^3.6.1",
    "notistack": "^2.0.2",
    "nprogress": "0.2.0",
    "parsleyjs": "^2.9.2",
    "popper.js": "^1.15.0",
    "prop-types": "15.7.2",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-custom-scroll": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.12.0",
    "react-icons": "4.3.1",
    "react-joyride": "^2.3.2",
    "react-modal": "3.14.3",
    "react-redux": "^7.2.6",
    "react-router": "5.2.1",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.3.0",
    "react-select": "^4.3.1",
    "redux": "4.1.2",
    "redux-api-middleware": "^3.2.1",
    "redux-saga": "1.1.3",
    "reselect": "^4.1.1",
    "sortablejs": "1.14.0",
    "urijs": ">=1.19.7",
    "yup": "^0.32.11"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "fetch"
    ],
    "ignore": [
      "/build"
    ]
  },
  "devDependencies": {
    "@rapper/ahooks": "^0.0.6",
    "@rapper/react-query": "^0.1.6",
    "@rapper/react-swr": "^0.0.5",
    "@rapper/request": "^0.0.7",
    "@tanstack/react-query": "^4.16.1",
    "@types/chart.js": "^2.9.34",
    "@types/classnames": "^2.3.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.8",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.176",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.11.6",
    "@types/nprogress": "^0.2.0",
    "@types/rc-tooltip": "^3.7.5",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.2",
    "@types/sortablejs": "^1.10.7",
    "@types/urijs": "^1.19.17",
    "@types/yup": "^0.29.13",
    "ahooks": "^3.7.2",
    "antd": "^4.19.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.40",
    "gh-pages": "^4.0.0",
    "npm-run-all": "4.1.5",
    "pre-commit": "^1.2.2",
    "react-scripts": "^5.0.0",
    "sass": "^1.43.4",
    "standard": "16.0.4",
    "swr": "^1.3.0",
    "typescript": "^4.4.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/adjacent-overload-signatures": "error",
          "@typescript-eslint/array-type": [
            "error",
            {
              "default": "array-simple"
            }
          ],
          "@typescript-eslint/ban-types": "error",
          "@typescript-eslint/consistent-type-assertions": "error",
          "@typescript-eslint/consistent-type-definitions": "off",
          "@typescript-eslint/explicit-member-accessibility": [
            "off",
            {
              "accessibility": "explicit"
            }
          ],
          "@typescript-eslint/indent": [
            "error",
            2,
            {
              "FunctionDeclaration": {
                "parameters": "first"
              },
              "FunctionExpression": {
                "parameters": "first"
              },
              "SwitchCase": 1,
              "MemberExpression": 1
            }
          ],
          "@typescript-eslint/interface-name-prefix": "off",
          "@typescript-eslint/member-delimiter-style": [
            "error",
            {
              "multiline": {
                "delimiter": "none",
                "requireLast": true
              },
              "singleline": {
                "delimiter": "semi",
                "requireLast": false
              }
            }
          ],
          "@typescript-eslint/member-ordering": "off",
          "@typescript-eslint/no-empty-function": "error",
          "@typescript-eslint/no-empty-interface": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/no-inferrable-types": [
            "error",
            {
              "ignoreParameters": true,
              "ignoreProperties": true
            }
          ],
          "@typescript-eslint/no-misused-new": "error",
          "@typescript-eslint/no-namespace": "error",
          "@typescript-eslint/no-parameter-properties": "off",
          "@typescript-eslint/no-redeclare": "off",
          "@typescript-eslint/no-require-imports": "off",
          "@typescript-eslint/no-this-alias": "off",
          "@typescript-eslint/no-use-before-declare": "off",
          "@typescript-eslint/no-var-requires": "off",
          "@typescript-eslint/prefer-for-of": "off",
          "@typescript-eslint/prefer-function-type": "off",
          "@typescript-eslint/prefer-namespace-keyword": "error",
          "@typescript-eslint/quotes": [
            "error",
            "single",
            {
              "allowTemplateLiterals": true
            }
          ],
          "@typescript-eslint/semi": [
            "error",
            "never"
          ],
          "@typescript-eslint/triple-slash-reference": "error",
          "@typescript-eslint/type-annotation-spacing": "error",
          "@typescript-eslint/unified-signatures": "error",
          "arrow-body-style": "off",
          "arrow-parens": [
            "off",
            "as-needed"
          ],
          "camelcase": "off",
          "capitalized-comments": "off",
          "complexity": "off",
          "constructor-super": "error",
          "curly": "error",
          "dot-notation": "off",
          "eol-last": "error",
          "eqeqeq": [
            "error",
            "smart"
          ],
          "guard-for-in": "error",
          "id-blacklist": "off",
          "id-match": [
            "error",
            "^(?!(any|Number|number|String|string|Boolean|boolean|Undefined|undefined)$).*$",
            {
              "onlyDeclarations": true
            }
          ],
          "import/order": "off",
          "import/no-mutable-exports": "off",
          "import/no-unused-modules": "off",
          "import/no-cycle": "off",
          "import/no-amd": "error",
          "max-classes-per-file": "off",
          "max-len": [
            "error",
            {
              "ignorePattern": "^import |^export \\{(.*?)\\}|^[ ]*export const |`.*`|\".*\"",
              "code": 150
            }
          ],
          "new-parens": "error",
          "no-bitwise": "error",
          "no-caller": "error",
          "no-cond-assign": "error",
          "no-console": "off",
          "no-debugger": "error",
          "no-empty": "error",
          "no-eval": "error",
          "no-fallthrough": "off",
          "no-import-side-effect": "off",
          "no-invalid-this": "off",
          "no-multiple-empty-lines": "error",
          "no-new-wrappers": "error",
          "no-null/no-null": "off",
          "no-shadow": [
            "off",
            {
              "hoist": "all"
            }
          ],
          "no-throw-literal": "error",
          "no-trailing-spaces": "error",
          "no-undef-init": "error",
          "no-underscore-dangle": [
            "off"
          ],
          "no-unsafe-finally": "error",
          "no-unused-expressions": "off",
          "no-unused-labels": "error",
          "no-var": "error",
          "object-shorthand": "off",
          "one-var": [
            "off",
            "never"
          ],
          "padding-line-between-statements": [
            "off",
            "error",
            {
              "blankLine": "always",
              "prev": "*",
              "next": "return"
            }
          ],
          "prefer-const": "error",
          "prefer-template": "off",
          "quote-props": "off",
          "radix": "error",
          "space-before-function-paren": [
            "error",
            {
              "anonymous": "never",
              "asyncArrow": "always",
              "named": "never"
            }
          ],
          "spaced-comment": [
            "error",
            "always",
            {
              "markers": [
                "/"
              ]
            }
          ],
          "use-isnan": "error",
          "valid-typeof": "off",
          "comma-dangle": [
            "error",
            {
              "arrays": "always-multiline",
              "objects": "always-multiline",
              "imports": "always-multiline",
              "exports": "always-multiline",
              "functions": "never"
            }
          ],
          "@typescript-eslint/typedef": "off",
          "react-hooks/rules-of-hooks": "error",
          "react-hooks/exhaustive-deps": "off",
          "react/jsx-indent": "off",
          "react/jsx-boolean-value": [
            "error",
            "always"
          ],
          "react/jsx-curly-spacing": [
            "error",
            {
              "when": "never"
            }
          ],
          "react/jsx-equals-spacing": [
            2,
            "never"
          ],
          "react/jsx-key": [
            2,
            {
              "checkFragmentShorthand": true
            }
          ],
          "react/no-string-refs": [
            2,
            {
              "noTemplateLiterals": true
            }
          ],
          "react/self-closing-comp": [
            "error",
            {
              "component": true,
              "html": false
            }
          ],
          "react/jsx-wrap-multilines": [
            2,
            {
              "declaration": "parens-new-line",
              "assignment": "parens-new-line",
              "return": "parens-new-line",
              "arrow": "parens-new-line",
              "condition": "ignore",
              "logical": "ignore",
              "prop": "ignore"
            }
          ],
          "react/no-this-in-sfc": "error",
          "react/no-unsafe": "error",
          "react/no-typos": "error",
          "react/display-name": "off",
          "react/prop-types": "off",
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
